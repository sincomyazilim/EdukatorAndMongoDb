        @await Html.PartialAsync("PartialSpiner.cshtml")
        <!-- Spinner End -->
        <!-- Sidebar Start -->
        @await Html.PartialAsync("PartialsideBar.cshtml")
        <!-- Sidebar End -->
        <!-- Content Start -->
        <div class="content">
            <!-- Navbar Start -->
            @await Html.PartialAsync("PartialNavbar.cshtml")
            <!-- Navbar End -->
            <div class="container-fluid pt-4 px-4">
                
                        @RenderBody()
                  
